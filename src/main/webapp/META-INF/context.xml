<?xml version="1.0" encoding="UTF-8" ?>
<Context path="/">
    <Resource
        name="jdbc/db"
        auth="Container"
        type="javax.sql.DataSource"
        driverClassName="org.sqlite.JDBC"
        url="jdbc:sqlite"
        closeMethod="close"
    />
    <Resource
            name="bean/auto-service"
            type="ru.nikolaeva.service.AutoService"
            factory="org.apache.naming.factory.BeanFactory"
    />

</Context>